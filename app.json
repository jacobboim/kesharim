{
  "expo": {
    "scheme": "kesharim",
    "name": "Kesharim",
    "slug": "Kesharim",
    "version": "1.0.10",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "url": "https://u.expo.dev/69c18aa6-da8e-4708-b07e-4573440c9030",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jacobboim.Kesharim",
      "buildNumber": "11"
    },
    "android": {
      "package": "com.jacobboim.Kesharim",
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },

    "androidStatusBar": {
      "hidden": false,
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },

    "androidNavigationBar": {
      "visible": "leanback"
    },

    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-navigation-bar",
        {
          "position": "relative",
          "visibility": "hidden",
          "behavior": "inset-swipe"
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "69c18aa6-da8e-4708-b07e-4573440c9030"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
